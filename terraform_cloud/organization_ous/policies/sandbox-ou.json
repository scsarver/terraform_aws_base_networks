{
  "Version": "2012-10-17",
  "Statement":[
    {
      "Sid": "AllowECTwo",
      "Effect": "Allow",
      "Action":[
        "ec2:*"
      ],
      "Resource":[
        "*"
      ]
    },
    {
      "Sid": "Denys3",
      "Effect": "Deny",
      "Action":[
        "s3:*"
      ],
      "Resource":[
        "*"
      ]
    },
    {
      "Sid": "DenyStopCloudTrail",
      "Effect": "Deny",
      "Action":[
        "cloudtrail:StopLogging"
      ],
      "Resource":[
        "arn:aws:cloudtrail:::trail/*"
      ]
    }
  ]
}
